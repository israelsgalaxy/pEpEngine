PEP DEFINITIONS ::=
BEGIN

Identity ::= SEQUENCE {
    address     UTF8String,
    fpr         UTF8String,
    user-id     UTF8String,
    username    UTF8String,
    comm-type   INTEGER (0..255),
    lang        PrintableString
}

Stringlist ::= SEQUENCE OF UTF8String

Version ::= SEQUENCE {
    major       INTEGER (0..255),
    minor       INTEGER (0..255)
}

Header ::= SEQUENCE {
    protocol    INTEGER (0..65535),
    version     Version,
    message     INTEGER (0..65535)
}

END

