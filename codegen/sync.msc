msc {

  # The entities
  ua [label="Alice\nUser"], a [label="Alice\nDevice"], b [label="Bob\nDevice"], ub [label="Bob\nUser"];

  # Small gap before the boxes
  |||;

  # Next four on same line due to ','
  a box a [label="Sole"],
  b box b [label="Sole"],
}
    Event CannotDecrypt: 
    send Beacon go Sole
    Event CannotDecrypt: 
    send SynchronizeGroupKeys go Grouped
    Event Beacon: 
    send Beacon go Sole
    send NegotiationRequest go Sole
