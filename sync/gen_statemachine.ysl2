include yslt.yml2

tstylesheet {
    template "/protocol/fsm"
    ||
    // state machine for «@name»

    #include "pEpEngine.h"


    void sm_«@name»(«@name»_state state)
    {
        switch (state) {
            `` apply "state"
        }
    }

    ||

    template "state"
    ||
    case «@name»:
        // bla
        break;

    ||
}


